package org.loop;

public class Test_1 {
	public static void main (String []args) {
        int i, j,count;
    
        System.out.println("Prime numbers betwwen 1 to 100  : "); 
        
		for(int a =2; a<=100; a++) {
			count = 0;
		for (j =a; j>=1;j--)	{
			if (a % j==0)
				count = count + 1;
		}
			if (count ==2 )
				System.out.println(""+a+"");
		}
				
	}
}               