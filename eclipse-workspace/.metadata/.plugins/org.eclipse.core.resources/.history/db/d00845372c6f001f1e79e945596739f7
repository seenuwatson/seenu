package map;

import java.util.HashMap;
import java.util.Iterator;
import java.util.LinkedHashMap;
import java.util.Map;
import java.util.Map.Entry;
import java.util.TreeMap;

public class MAPLINKEDHASHMAP {
	public static void main(String[] args) {
		
		Map<String,Integer> s=new LinkedHashMap<>();
		
		s.put("csk", 1);
		s.put("mi", 2);
		s.put("rcb", 3);
		s.put("srh", null);
		s.put("kkr", 4);
		
		System.out.println(s);
		
		Map<String,Integer> s1=new HashMap<>();
		
		s1.put("csk", 1);
		s1.put("mi", 2);
		s1.put("rcb", 3);
		s1.put("srh", null);
		System.out.println(s1);
	
		Iterator<Entry<String, Integer>> iterator = s1.entrySet().iterator();
		while(iterator.hasNext()) {
			System.out.println(iterator.next());
		}

	Map<Integer, String> s2=new TreeMap<>();
		
		s2.put(3, "csk");
		s2.put(8, "mi");
		s2.put(1, "rr");
		s2.put(2, "RCB");
		s2.put(4, "gt");
	//	s2.put(null," dc");
		
		System.out.println(s2);
		
		for(Map.Entry<String, Integer>entry: s.entrySet()){
			System.out.println(entry.getKey()+":"+entry.getValue());
		}
		System.out.println("*******");
	 Iterator<Integer> iterator2 = s2.keySet().iterator();
	 Iterator<String> iterator3 = s2.values().iterator();
	 while(iterator2.hasNext()) {
	      Integer key = iterator2.next();
		 }
	 
	 
	}

	

}
