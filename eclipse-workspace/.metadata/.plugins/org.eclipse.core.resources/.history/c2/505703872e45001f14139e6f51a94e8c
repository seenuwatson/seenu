package com.cricket;

public class CricketDetails {
	
         private void cricketTotalPlayers() {
        	 System.out.println("11");
         }
     
         private void cricketBatman() {
           System.out.println("2");
         }
	   private void cricketBountry() {
		   System.out.println("4,6");	
	   }
	
	
	public static void mani(String[] argm ) { 
		
		CricketDetails rocket =new CricketDetails();
				
		rocket.cricketBatman();	
		rocket.cricketBountry();
	    rocket.cricketTotalPlayers();
	}
 
	
}